# http://editorconfig.org
# 部分编辑器可能需要安装对应的插件才可以支持该配置。
# 例如 VSCode 需要安装 EditorConfig for VSCode 扩展 。
root = true

[*]
charset = utf-8
end_of_line = lf
indent_size = 2
indent_style = space
insert_final_newline = true
max_line_length = 80
trim_trailing_whitespace = true

[*.md]
max_line_length = 0
trim_trailing_whitespace = false
